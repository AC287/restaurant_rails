<%= form_for @user do |f| %>

  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :password %>
  <%= f.text_field :password_digest %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :role %>
  <%= f.text_field :role %>

  <%= f.label :img_url %>
  <%= f.text_field :img_url %>
  <br>
  <%= f.submit %>

<% end %>
